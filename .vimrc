set number
set ruler
set shiftwidth=4
set tabstop=4
set backspace=2
syntax on
filetype indent on
set autoindent
set nobackup
set hlsearch
set showcmd
set encoding=utf-8
set fileencoding=utf-8
set t_Co=256
set virtualedit=onemore
highlight Comment ctermfg=green
set vb
set t_vb=

let g:go_highlight_trailing_whitespace_error=0

